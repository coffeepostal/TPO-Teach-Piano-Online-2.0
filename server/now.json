{
    "version": 2,
    "builds": [
        {
            "src": "/start.js",
            "use": "@now/node-server"
        },
        {
            "src": "/public/**",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        },
        {
            "src": "/public/(.*)",
            "dest": "/$1"
        },
        {
            "src": "/files/email.csv",
            "dest": "/email.csv"
        }
    ]
}